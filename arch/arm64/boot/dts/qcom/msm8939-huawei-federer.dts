&pm8916_bms {
	status = "okay";

	monitored-battery = <&battery>;
	//power-supplies = <&charger>;
};

&pm8916_codec {
	qcom,micbias-lvl = <2800>;
	qcom,mbhc-vthreshold-low = <75 150 237 450 500>;
	qcom,mbhc-vthreshold-high = <75 150 237 450 500>;
	qcom,micbias1-ext-cap;
	qcom,hphl-jack-type-normally-open;
	status = "okay";
};

&pm8916_mpps {
	pinctrl-names = "default";
	pinctrl-0 = <&pm8916_mpp2_in &pm8916_mpp4_in>;

	pm8916_mpp2_in: pm8916-mpp2-in-state {
		pinconf {
			pins = "mpp2";
			function = "analog"; //input
			qcom,amux-route = <1>; //PMIC_MPP_AMUX_ROUTE_CH6
		};
	};

	pm8916_mpp4_in: pm8916-mpp4-in-state {
		pinconf {
			pins = "mpp4";
			function = "analog"; //input
			qcom,amux-route = <3>; //PMIC_MPP_AMUX_ROUTE_CH8
		};
	};
};

&pm8916_resin {
	linux,code = <KEY_VOLUMEDOWN>;
	status = "okay";
};

&pm8916_rpm_regulators {
	pm8916_l6: l6 {
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on; 
		regulator-boot-on;
	};

	pm8916_l8: l8 {
		regulator-min-microvolt = <2950000>;
		regulator-max-microvolt = <2950000>;
	};

	pm8916_l16: l16 {
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-boot-on;
	};

	pm8916_l17: l17 {
		regulator-min-microvolt = <2850000>;
		regulator-max-microvolt = <2850000>;
		regulator-always-on; 
		regulator-boot-on;
	};
};

&pm8916_vadc {
	channel@2 {
		reg = <VADC_VCHG_SNS>;
		//reg = <0x2>;
		qcom,pre-scaling = <10 81>;
		label = "ireg_fb";
	};
	channel@5 {
		reg = <VADC_VCOIN>;
		qcom,pre-scaling = <1 3>;
		label = "vcoin";
	};
	channel@6 {
		reg = <VADC_VBAT_SNS>;
		//reg = <0x6>;
		//reg = <VADC_VBAT_SNS>;
		qcom,pre-scaling = <1 3>;
		label = "vbat_sns";
	};
	/* cam therm */
	channel@11 {
		reg = <VADC_P_MUX2_1_1>;
		//qcom,decimation = <512>;
		qcom,pre-scaling = <1 1>;
		//qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		label = "mpp2_div1";
	};
	/* cpu therm */
	channel@13 {
		reg = <VADC_P_MUX4_1_1>;
		//qcom,decimation = <512>;
		qcom,pre-scaling = <1 1>;
		qcom,hw-settle-time = <200>;
		label = "mpp4_div1";
	};
	channel@30 {
		reg = <VADC_LR_MUX1_BAT_THERM>;
		//reg = <0x30>;
		qcom,pre-scaling = <1 1>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		qcom,avg-samples = <1>;
		label = "batt_therm";
	};
	channel@31 {
		//reg = <0x31>;
		reg = <VADC_LR_MUX2_BAT_ID>;
		qcom,pre-scaling = <1 1>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		label = "batt_id";
	};
	channel@32 {
		reg = <VADC_LR_MUX3_XO_THERM>;
		//reg = <0x32>;
		qcom,pre-scaling = <1 1>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		label = "xo_therm";
	};
	/* pa therm0 */
	channel@36 {
		reg = <VADC_LR_MUX7_HW_ID>;
		//reg = <0x36>;
		qcom,pre-scaling = <1 1>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		label = "pa_therm0";
	};
	channel@3c {
		reg = <VADC_LR_MUX3_BUF_XO_THERM>;
		//reg = <0x3c>;
		qcom,pre-scaling = <1 1>;
		qcom,ratiometric;
		qcom,hw-settle-time = <200>;
		label = "xo_therm_buf";
	};
	channel@b {
		reg = <VADC_CHG_TEMP>;
		//reg = <0xb>;
		qcom,pre-scaling = <1 1>;
		qcom,hw-settle-time = <0>;
		label = "chg_temp";
	};
};

&pm8916_vib {
	status = "okay";
};

&sdhc_1 {
	status = "okay";
};

&sdhc_2 {
	pinctrl-0 = <&sdc2_default &sdc2_cd_default>;
	pinctrl-1 = <&sdc2_sleep &sdc2_cd_default>;
	pinctrl-names = "default", "sleep";

	cd-gpios = <&tlmm 38 GPIO_ACTIVE_LOW>;

	status = "okay";
};

&sound {
	model = "wt86528";
	audio-routing =
		"AMIC1", "MIC BIAS External1",
		"AMIC2", "MIC BIAS External2",
		"AMIC3", "MIC BIAS External1";

	pinctrl-0 = <&cdc_pdm_default &sec_mi2s_default>;
	pinctrl-1 = <&cdc_pdm_sleep &sec_mi2s_sleep>;
	pinctrl-names = "default", "sleep";

	status = "okay";

	dai-link-primary {
		link-name = "Primary MI2S";
		cpu {
			sound-dai = <&lpass MI2S_PRIMARY>;
		};
		codec {
			sound-dai = <&lpass_codec 0>, <&pm8916_codec 0>;
		};
	};

	dai-link-tertiary {
		link-name = "Tertiary MI2S";
		cpu {
			sound-dai = <&lpass MI2S_TERTIARY>;
		};
		codec {
			sound-dai = <&lpass_codec 1>, <&pm8916_codec 1>;
		};
	};
};

